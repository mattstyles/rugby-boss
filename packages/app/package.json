{
  "name": "rugby-boss",
  "private": true,
  "version": "0.0.0",
  "description": "Rugby management simulation",
  "main": "dist/index.html",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production parcel build src/index.html",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "DEBUG=true parcel src/index.html"
  },
  "babel": {
    "presets": [
      "urban"
    ],
    "plugins": [
      "babel-plugin-styled-components",
      "transform-react-require",
      "lodash",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattstyles/rugby-boss.git"
  },
  "keywords": [
    "raid",
    "react",
    "game",
    "rugby"
  ],
  "author": "mattstyles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattstyles/rugby-boss/issues"
  },
  "homepage": "https://mattstyles.github.io/rugby-boss",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-react-require": "^1.0.1",
    "babel-preset-urban": "^1.0.0",
    "create-raid-app": "2.13.0",
    "gh-pages": "^1.2.0",
    "minimist": "^1.2.3",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "parcel-bundler": "^1.9.7",
    "rimraf": "^2.6.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "eventemitter3": "^2.0.3",
    "lodash": "^4.17.10",
    "most": "^1.7.3",
    "open-color": "^1.6.3",
    "prop-types": "^15.6.2",
    "radios": "*",
    "raid": "^2.13.0",
    "raid-addons": "^2.13.0",
    "raid-fl": "^2.13.0",
    "raid-navigator": "^2.13.0",
    "raid-streams": "^2.13.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "reselect": "^2.5.4",
    "styled-components": "^3.4.2"
  }
}
